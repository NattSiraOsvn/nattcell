{
  "cell_id": "cell:rbac",
  "cell_name": "rbac-cell",
  "version": "1.0.0",
  "description": "Kernel cell for Role-Based Access Control",
  "domain": "SYSTEM",
  "layer": 0,
  "layer_name": "KERNEL",
  "capabilities": ["RBAC_CHECK", "ROLE_ASSIGN", "ROLE_REVOKE", "PERMISSION_MANAGE"],
  "emits": ["rbac.role.assigned", "rbac.role.revoked", "rbac.permission.granted", "rbac.access.denied"],
  "authoritative_events": ["rbac.*"],
  "subscribes": ["system.bootstrap", "system.shutdown"],
  "dependencies": ["cell:config"],
  "constitutional_state": "STATE_1",
  "approved_by": "GATEKEEPER"
}
